{{#each randomBeer}}
    <img src="{{image_url}}" alt="beer image">
    <h1>{{name}}</h1>
    <p> {{description}} </p>
    <p> {{tagline}} </p>
    <h2>Food Pairing</h2>
    
    <ul>
        {{#each food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>

    <p> <strong>Brewers Tips: </strong> {{brewers_tips}}</p>
{{/each}}